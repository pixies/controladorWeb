#NameVirtualHost *:80
<VirtualHost *>
   ServerName projeto-alavanca.org
   ServerAlias www.projeto-alavanca.org
   ServerAdmin walrusATvideo.if.usp.br
   DocumentRoot /aletta/sitios/projeto-alavanca.org
   ErrorLog /var/log/httpd/projeto-alavanca.org_error_log
   CustomLog /var/log/httpd/projeto-alavanca.org_access_log commom
   <Directory "/aletta/sitios/projeto-alavanca.org">
        Options FollowSymLinks 
        AllowOverride All
   </Directory>
</VirtualHost>
