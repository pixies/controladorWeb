#NameVirtualHost *:80
<VirtualHost *>
   ServerName contratv.net
   ServerAlias www.contratv.net
   ServerAlias contratv.net
   ServerAdmin ruiz@descentro.org
   DocumentRoot /aletta/sitios/contratv.net/htdocs-prod
   ErrorLog /var/log/httpd/contratv.net_error_log
   CustomLog /var/log/httpd/contratv.net_access_log commom
   <Directory "/aletta/sitios/contratv.net/htdocs-prod">
        Options FollowSymLinks 
        AllowOverride None
   </Directory>
</VirtualHost>
