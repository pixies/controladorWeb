#NameVirtualHost *:80
<VirtualHost *>
   ServerName www.semussum.org
   ServerAlias www.www.semussum.org
   ServerAlias www.semussum.org
   ServerAdmin kram3r@gmail.com
   DocumentRoot /aletta/sitios/www.semussum.org/htdocs-prod
   ErrorLog /var/log/httpd/www.semussum.org_error_log
