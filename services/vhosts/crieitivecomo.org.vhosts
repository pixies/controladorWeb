#NameVirtualHost *:80
<VirtualHost *>
   ServerName crieitivecomo.org
   ServerAlias www.crieitivecomo.org
   ServerAdmin ruiz@descentro.org
   DocumentRoot /aletta/sitios/crieitivecomo.org/htdocs-prod
   ErrorLog /var/log/httpd/crieitivecomo.org_error_log
   CustomLog /var/log/httpd/crieitivecomo.org_access_log commom
   <Directory "/aletta/sitios/crieitivecomo.org/htdocs-prod">
        Options FollowSymLinks 
        AllowOverride All
   </Directory>
</VirtualHost>
