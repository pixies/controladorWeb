#NameVirtualHost *:80
<VirtualHost *>
   ServerName retomeatecnonologia.info
   ServerAlias www.retomeatecnonologia.info
   ServerAlias retomeatecnonologia.info
   ServerAdmin tati@descentro.org
   DocumentRoot /aletta/sitios/retomeatecnonologia.info/htdocs-prod
   ErrorLog /var/log/httpd/retomeatecnonologia.info_error_log
   CustomLog /var/log/httpd/retomeatecnonologia.info_access_log commom
   <Directory /aletta/sitios/retomeatecnonologia.info/htdocs-prod>
        Options FollowSymLinks
        AllowOverride None
   </Directory>
</VirtualHost>
