#NameVirtualHost *:80
<VirtualHost *>
   ServerName midiatatica.org
   ServerAlias www.midiatatica.org
   ServerAlias midiatatica.org
   ServerAdmin ruiz@descentro.org
   DocumentRoot /aletta/sitios/midiatatica.org/htdocs-prod
   ErrorLog /var/log/httpd/midiatatica.org_error_log
   CustomLog /var/log/httpd/midiatatica.org_access_log commom
   <Directory /aletta/sitios/midiatatica.org/htdocs-prod>
        Options FollowSymLinks
        AllowOverride None
   </Directory>
</VirtualHost>
